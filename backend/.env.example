# Example environment variables for the Pixel Art Canvas Backend
# Copy this file to .env and replace the placeholder values with your actual secrets.

# Your Google Cloud Project OAuth 2.0 Client ID for Web application
GOOGLE_CLIENT_ID="YOUR_GOOGLE_CLIENT_ID_HERE"

# Your MongoDB Atlas connection string
MONGODB_URI="YOUR_MONGODB_CONNECTION_STRING_HERE"

# Optional: Specify the port the server should run on (defaults to 3001 if not set)
PORT=3000 